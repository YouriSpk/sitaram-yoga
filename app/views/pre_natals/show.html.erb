<div class="container mt-5">
  <div class="row">
    <div class="col-md-8 offset-md-2">
      <p class="text-success"><%= notice %></p>

      <%= render @pre_natal %>

      <div class="text-center mt-4">
        <%= link_to "Edit this pre natal", edit_pre_natal_path(@pre_natal), class: 'btn btn-warning' %> |
        <%= link_to "Back to pre natals", pre_natals_path, class: 'btn btn-secondary' %>

        <%= button_to "Destroy this pre natal", @pre_natal, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %>
      </div>
    </div>
  </div>
</div>
